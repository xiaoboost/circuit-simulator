{
  "name": "@circuit/math",
  "description": "circuit's math",
  "version": "1.0.0",
  "author": "xiaoboost",
  "license": "MIT",
  "source": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "ava --no-cache",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/circuit-simulator.git"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "environmentVariables": {
      "TS_NODE_PROJECT": "tsconfig.json"
    },
    "files": [
      "tests/**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@xiao-ai/utils": "^1.2.2",
    "bignumber.js": "^9.0.1"
  },
  "devDependencies": {
    "@circuit/tsconfig": "workspace:*",
    "@types/circuit": "workspace:*",
    "ava": "^3.15.0",
    "eslint": "^7.27.0",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  }
}
