{
  "name": "circuit-simulator",
  "version": "2.0.0-alpha",
  "description": "在线电路仿真系统",
  "scripts": {
    "ts-node": "node node_modules/ts-node/dist/bin.js -P ./build/tsconfig.json --transpile-only",
    "dev": "cross-env NODE_ENV=development npm run ts-node ./build/webpack.dev.ts",
    "build": "cross-env NODE_ENV=production npm run ts-node ./build/webpack.prod.ts",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ./.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ./.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "test": "ava --no-cache"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "tsconfig-paths/register",
      "./tests/setups/env.ts"
    ],
    "environmentVariables": {
      "TS_NODE_PROJECT": "tsconfig.json"
    },
    "files": [
      "tests/**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@antv/g2": "^4.1.16",
    "antd": "^4.15.4",
    "bignumber.js": "^9.0.1",
    "core-js": "^3.11.2",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.6.0",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/generate-json-webpack-plugin": "^0.3.3",
    "@types/html-webpack-plugin": "^3.2.5",
    "@types/koa": "^2.13.1",
    "@types/mime": "^2.0.3",
    "@types/mini-css-extract-plugin": "^1.4.2",
    "@types/node": "^15.0.1",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-transition-group": "^4.4.1",
    "@types/webpack-bundle-analyzer": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "ava": "^3.15.0",
    "browser-env": "^3.3.0",
    "chalk": "^4.1.1",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "esbuild-loader": "^2.12.0",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "generate-json-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.3.1",
    "koa": "^2.13.1",
    "memfs": "^3.2.2",
    "mime": "^2.5.2",
    "mini-css-extract-plugin": "^1.6.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0",
    "ts-loader": "^9.1.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "typescript-plugin-css-modules": "^3.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.2",
    "webpack-bundle-analyzer": "^4.4.1"
  },
  "engines": {
    "node": ">= 9.0.0",
    "npm": ">= 6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/circuit-simulator.git"
  }
}
